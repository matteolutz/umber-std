static mut NEXT_RAND: number = 0;

fun rand(): number {
    NEXT_RAND = NEXT_RAND * 1103515245 + 12345;
    return (NEXT_RAND / 65536) % 32768;
};

fun rand_range(min: number, max: number): number {
    return rand() % (max + 1 - min) + min;
};

fun srand(seed: number): void {
	NEXT_RAND = seed;
	return;
};
